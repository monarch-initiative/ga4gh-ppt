<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="705.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="53.0" prefWidth="705.0">
      <children>
         <Label text="Proband ID">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
         </Label>
         <TextField fx:id="probandIdTextField">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
         </TextField>
         <RadioButton fx:id="sexM" mnemonicParsing="false" text="M">
            <HBox.margin>
               <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
            </HBox.margin>
         </RadioButton>
         <RadioButton fx:id="sexF" mnemonicParsing="false" text="F">
            <HBox.margin>
               <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
            </HBox.margin>
         </RadioButton>
         <RadioButton fx:id="sexO" mnemonicParsing="false" text="O">
            <HBox.margin>
               <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
            </HBox.margin>
         </RadioButton>
         <RadioButton fx:id="sexU" mnemonicParsing="false" text="U">
            <HBox.margin>
               <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
            </HBox.margin>
         </RadioButton>
         <Button fx:id="onsetButton" mnemonicParsing="false" text="Onset">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
         </Button>
         <Button fx:id="lastEncounterButton" mnemonicParsing="false" text="Last encounter">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
         </Button>
      </children>
   </HBox>
</VBox>
